{
  "name": "roshan-ui",
  "description": "Fast and modern UI library for Vue 3",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./style/*": "./dist/style/*",
    "./windicss/*": "./dist/windicss/*",
    "./windicss": {
      "import": "./dist/windicss/index.mjs",
      "require": "./dist/windicss/index.js",
      "types": "./dist/windicss/windicss.d.ts"
    }
  },
  "scripts": {
    "storybook": "start-storybook --no-open -p 6006",
    "build-storybook": "build-storybook",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx,.vue",
    "build": "tsx scripts/build.ts"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vueuse/core": "^9.2.0",
    "defu": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@iconify-json/ant-design": "^1.1.3",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-backgrounds": "^6.5.10",
    "@storybook/addon-docs": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-measure": "^6.5.10",
    "@storybook/addon-outline": "^6.5.10",
    "@storybook/addons": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/channel-postmessage": "^6.5.10",
    "@storybook/channel-websocket": "^6.5.10",
    "@storybook/client-api": "^6.5.10",
    "@storybook/preview-web": "^6.5.10",
    "@storybook/vue3": "^6.5.10",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/test-utils": "^2.0.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "happy-dom": "^6.0.4",
    "terser": "^5.15.0",
    "unplugin-icons": "^0.14.9",
    "vite": "^3.1.0",
    "vite-plugin-chunk-split": "^0.2.7",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-windicss": "^1.8.8",
    "vitest": "^0.23.2",
    "vue": "^3.2.39",
    "vue-eslint-parser": "^9.1.0",
    "windicss": "^3.5.6"
  },
  "peerDependencies": {
    "vue": "^3.2.38"
  }
}
